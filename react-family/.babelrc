{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        "react-hot-loader/babel",
        ["import", {
            "libraryName": "antd",
            "libraryDirectory": "es",
            "style": "css" // `style: true` 会加载 less 文件
          }],
        [
            "@babel/plugin-proposal-decorators",
            {"legacy": true}
        ],
        [
            "@babel/plugin-proposal-class-properties",
            {"loose": true}     
        ],
        "@babel/plugin-transform-runtime" // TODO:@babel/plugin-transform-runtime
    ]
}